{
  "name": "SchemaObjectTest",
  "servicePath": "schemaObjectTest/v1/",
  "schemas": {
    "Foo": {
      "id": "Foo",
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "description": "Primary key."
        },
        "bar": {
          "type": "string",
          "description": "Foo's favorite bar."
        },
        "baz": {
          "type": "integer",
          "description": "It's spelled buzz."
        },
        "qux": {
          "type": "string",
          "description": "Not what it seems.",
          "format": "int64"
        },
        "quux": {
          "type": "array",
          "description": "The plural of qux",
          "items": {
            "type": "string",
            "format": "double"
          }
        },
        "corge": {
          "type": "number",
          "description": "A double field that's serialized as a number.",
          "format": "double"
        }
      }
    },
    "Bar": {
      "id": "Bar",
      "type": "object",
      "properties": {
        "primary": {
          "$ref": "Foo",
          "description": "The primary foo."
        },
        "foos": {
          "type": "array",
          "description": "A bunch of foos.",
          "items": {
            "$ref": "Foo"
          }
        }
      }
    },
    "Context": {
     "id": "Context",
     "type": "object",
     "properties": {
      "facets": {
       "type": "array",
       "items": {
        "type": "array",
        "items": {
         "type": "object",
         "properties": {
          "anchor": {
           "type": "string"
          },
          "label": {
           "type": "string"
          },
          "label_with_op": {
           "type": "string"
          }
         }
        }
       }
      }
     }
    },
    "-some-entity-": {
      "id": "-some-entity-",
      "type": "object",
      "properties": {
        "%badly#named property~!@#$%^&*()?": {
          "type": "string",
          "format": "int64"
        }
      }
    }
  },
  "resources": {
    "-some-resource-": {
      "methods": {
        "-some-method-": {
          "id": "service.foos.get",
          "path": "foos/{fooId}",
          "response": {
            "$ref": "-some-entity-"
          },
          "httpMethod": "GET",
          "parameters": {
            "-path param-": {
              "type": "integer",
              "location": "path"
            },
            "-query param-": {
              "type": "integer",
              "location": "path"
            }
          },
          "parameterOrder": ["-path param-"]
        }
      }
    }
  }
}
